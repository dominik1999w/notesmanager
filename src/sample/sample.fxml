<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootBox" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <SplitPane dividerPositions="0.3066132264529058" focusTraversable="true" VBox.vgrow="ALWAYS">
      <items>
        <AnchorPane style="-fx-background-color: grey;">
          <children>
            <TreeView fx:id="FilesView" onMouseClicked="#displayFile" prefHeight="542.0" prefWidth="271.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="33.0" />
            <Button fx:id="newFileButton" layoutX="0.0" layoutY="0.0" mnemonicParsing="false" onAction="#clickCategoryControllerButton" prefHeight="33.0" style="-fx-background-color: white;" text="new file" underline="false" />
            <Button fx:id="removeButton" layoutX="73.0" layoutY="0.0" mnemonicParsing="false" onAction="#removeFile" prefHeight="33.0" prefWidth="72.9998779296875" style="-fx-background-color: white;" text="remove" underline="false" wrapText="true">
              <font>
                <Font size="13.0" />
              </font>
            </Button>
            <Button fx:id="newCategoryButton" layoutX="230.0" layoutY="0.0" mnemonicParsing="false" onAction="#clickNewCategoryButton" prefHeight="33.0" prefWidth="73.0001220703125" style="-fx-background-color: white;" text="new cat" />
          </children>
        </AnchorPane>
        <AnchorPane>
          <children>
            <TextArea fx:id="DisplayFileText" layoutY="51.0" prefHeight="647.0" prefWidth="686.0" style="-fx-background-color: grey;" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="51.0" />
                  <AnchorPane prefHeight="50.0" prefWidth="686.0">
                     <children>
                  <TextArea fx:id="DisplayTitle" editable="false" layoutX="81.0" prefHeight="50.0" prefWidth="605.0" wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="81.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                        <Button fx:id="FileNatively" layoutX="14.0" layoutY="9.0" mnemonicParsing="false" onMouseClicked="#OpenFileNatively" text="Button" />
                     </children>
                  </AnchorPane>
          </children>
        </AnchorPane>
      </items>
    </SplitPane>
  </children>
  <stylesheets>
    <URL value="@sample.css" />
  </stylesheets>
</VBox>
