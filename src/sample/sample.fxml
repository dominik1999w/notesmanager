<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="rootBox" prefHeight="700.0" prefWidth="1000.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <SplitPane dividerPositions="0.3066132264529058" focusTraversable="true" VBox.vgrow="ALWAYS">
      <items>
        <AnchorPane style="-fx-background-color: grey;">
          <children>
            <TreeView fx:id="FilesView" onMouseClicked="#displayFile" prefHeight="542.0" prefWidth="271.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="33.0" />
            <Button fx:id="newFileButton" layoutX="0.0" layoutY="0.0" mnemonicParsing="false" onAction="#clickCategoryControllerButton" prefHeight="33.0" style="-fx-background-color: white;" text="new file" underline="false" />
            <Button fx:id="removeButton" layoutX="73.0" layoutY="0.0" mnemonicParsing="false" onAction="#removeFile" prefHeight="33.0" prefWidth="73.0" style="-fx-background-color: white;" text="remove" underline="false" wrapText="true">
              <font>
                <Font size="13.0" />
              </font>
            </Button>
          </children>
        </AnchorPane>
        <AnchorPane>
          <children>
            <TextArea fx:id="DisplayFileText" prefHeight="665.0" prefWidth="689.0" style="-fx-background-color: grey;" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="33.0" />
            <TextArea fx:id="DisplayTitle" editable="false" layoutX="0.0" layoutY="0.0" prefHeight="33.0" prefWidth="698.0" wrapText="true" />
          </children>
        </AnchorPane>
      </items>
    </SplitPane>
  </children>
  <stylesheets>
    <URL value="@sample.css" />
  </stylesheets>
</VBox>
